global {
    usage-count no;
}

common {
    protocol B;

    startup {
        wfc-timeout 60;
        degr-wfc-timeout 60;
        outdated-wfc-timeout 60;
    }

    disk {
        al-extents 3389;
        disk-barrier no;
        disk-flushes no;
        on-io-error detach;
        resync-rate 100M;
    }

    net {
        after-sb-0pri discard-zero-changes;
        after-sb-1pri consensus;
        after-sb-2pri disconnect;
        max-buffers 8000;
        max-epoch-size 8000;
        ping-timeout 200;
        rr-conflict disconnect;
        sndbuf-size 0;
        timeout 100;
    }
}